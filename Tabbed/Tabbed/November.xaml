<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tabbed.November"
             Title="November">
    <ContentPage.Content>
        <StackLayout>
            <Button Text = "Hingedepäe" FontSize="Large" BorderWidth="1"
        HorizontalOptions="Center" VerticalOptions="CenterAndExpand"
        Clicked="Button_Clicked" />
            <Button Text = "Isadepäev" FontSize="Large" BorderWidth="1"
        HorizontalOptions="Center" VerticalOptions="CenterAndExpand"
        Clicked="Button_Clicked_1" />
            <Button Text = "Taassünni päev" FontSize="Large" BorderWidth="1"
        HorizontalOptions="Center" VerticalOptions="CenterAndExpand"
        Clicked="Button_Clicked_2" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>